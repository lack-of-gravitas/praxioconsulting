{
  "name": "praxioconsulting",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "lint": "next lint",
    "prettier-fix": "prettier --write .",
    "find:unused": "npx next-unused",
    "stripe:listen": "stripe listen --forward-to=localhost:3000/api/webhooks --project-name=saas-starter"
  },
  "sideEffects": false,
  "dependencies": {
    "@builder.io/partytown": "^0.5.2",
    "@radix-ui/react-dropdown-menu": "^0.1.6",
    "@react-spring/web": "^9.4.4",
    "@stripe/stripe-js": "^1.29.0",
    "@supabase/supabase-auth-helpers": "^1.3.0",
    "autoprefixer": "^10.4.5",
    "body-scroll-lock": "^4.0.0-beta.0",
    "classnames": "2.3.1",
    "clsx": "^1.1.1",
    "email-validator": "^2.0.4",
    "js-cookie": "^3.0.1",
    "keen-slider": "^6.6.11",
    "lodash.random": "^3.2.0",
    "lodash.throttle": "^4.1.1",
    "next": "^12.1.5",
    "next-themes": "^0.1.1",
    "postcss-nesting": "^10.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-fast-marquee": "^1.3.1",
    "react-merge-refs": "^1.1.0",
    "react-use-measure": "^2.1.1",
    "stripe": "^8.219.0",
    "swr": "^1.3.0",
    "tabbable": "^5.3.1",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.5",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/classnames": "2.3.1",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash.random": "^3.2.7",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^17.0.30",
    "@types/react": "^17.0.38",
    "eslint": "^8.14.0",
    "eslint-config-next": "^12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.13",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.4.4",
    "prettier": "^2.6.2",
    "typescript": "4.6.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  }
}
