{
  "name": "praxio",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "site"
  ],
  "scripts": {
    "build": "turbo run build --scope=praxioconsulting --include-dependencies --no-deps",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "types": "turbo run types",
    "prettier-fix": "prettier --write ."
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "prettier": "^2.6.2",
    "turbo": "^1.2.14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "turbo run lint"
    }
  },
  "packageManager": "yarn@1.22.17"
}
